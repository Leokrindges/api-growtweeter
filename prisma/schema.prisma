generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @db.Uuid @default(uuid())
  name String @db.VarChar(255)
  email String @db.VarChar(255) @unique
  username String @db.VarChar(255)
  password String @db.VarChar(255)

  @@map("users")
}
